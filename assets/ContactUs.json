{
  "schema": {
    "type": "object",
    "title": "Contact Us",
    "properties": {
      "NAME": {
        "title": "Clinic Name",
        "type": "string"
      },
      "CONTACT": {
        "title": "Contact Person",
        "type": "string"
      },
      "PHONE": {
        "title": "Phone",
        "type": "string"
      },
      "EMAIL": {
        "title": "Email",
        "type": "string",
        "pattern": "^\\S+@\\S+$"
      },
      "MESSAGE": {
        "title": "message",
        "type": "string"
      }
    },
    "required": [
      "NAME",
      "CONTACT",
      "EMAIL",
      "MESSAGE"
    ]
  },
  "form": [
