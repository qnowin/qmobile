    "CONTACT",
    "PHONE",
    "MESSAGE",
    "EMAIL",
    {
      "type": "submit",
      "style": "btn-info",
      "title": "Submit"
    }
  ],
  "layout": [{
      "type": "flex",
      "flex-flow": "row wrap",
      "items": ["NAME"]
    },
    {
      "type": "flex",
      "flex-flow": "row wrap",
      "items": ["CONTACT"]
    },
    {
      "type": "flex",
      "flex-flow": "row wrap",
      "items": ["PHONE"]
    },
    {
      "type": "flex",
      "flex-flow": "row wrap",
      "items": ["EMAIL"]
    },
    {
      "type": "flex",
      "flex-flow": "row wrap",
      "items": ["MESSAGE"]
    }
  ],
  "data": {
    "NAME": "",
    "CONTACT": "",
    "PHONE": "",
    "EMAIL": "",
    "MESSAGE": ""
  }
}
